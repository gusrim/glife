if $ARGS[0] = 'makePrefTmp':
	if pcs_makeup ! 0 and makePrefTmp ! 0:
		if haraklover[lover_number] = 0:

			if pcs_hairbsh = 0:
				loverrelation[lover_number] -= 1
				$meetMake = '<<$loverdesc[lover_number]>> says timidly "I guess you had to leave in a hurry huh?"'
