# LOCA

if Enable_settingmode!2: killvar '$cheatmenu'

if mod_list > 0:
	mod_i=1
	:mod_exec
	if $mod_name[mod_i] ! '':
		gs 'mod_<<$mod_name[mod_i]>>', $ARGS[0], $ARGS[1], $ARGS[2], $ARGS[3], $ARGS[4], $ARGS[5], $ARGS[6], $ARGS[7], $ARGS[8]
	end
	
	if mod_list > mod_i:
		mod_i+=1
		jump 'mod_exec'
	end
end

--- LOCA ---------------------------------

