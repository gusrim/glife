# rolanapt

if $ARGS[0] = 'home':
	music_loop = 0
	CLOSE ALL
	if sound = 0:PLAY 'sound/korr.mp3',30
	$metkaM = 'home'
	$locM = 'rolanapt'
	$metka = 'home'
	$loc = 'rolanapt'
	$location_type = 'private'
	clr
	popolaini = 0
	saunaYouRoom = 0
	boycherdaksex = 0
	minut += 1
	bonusZ = 0
	gs 'stat'

	cla
	*clr
	'<center><img src="images/locations/pavlovsk/resident/apartment/aptdoor.jpg"></center>'
	'You walk up to the front door and knock.'
	if hour >= 23:
		'It''s too late in the day to visit Mr. Matveev'
		act 'Move away from the door':gt 'pavComplexb3', 'etaj_1'
	elseif hour < 15:
		'Probably Mr. Matveev is on his duty.'
		act 'Move away from the door':gt 'pavComplexb3', 'etaj_1'
	elseif newbath = 1 and rolanworker = 3 and hour >= 15 and hour < 23 and RolanLoc[hour] >= 2 and RolanLoc[hour] =< 8:
		'You can''t wait to see Rolan''s face.'
		act 'ring the bell':gt 'rolanbath', 'surprise'	
	elseif hour >= 15 and hour < 23 and RolanLoc[hour] >= 2 and RolanLoc[hour] =< 8:
		*clr & cla
		'<center><img src="images/locations/pavlovsk/resident/apartment/aptrolan/rolanwelcome.jpg"></center>'		
		'The door open: "Welcome Mrs <<$pcs_lastname>>, here take a seat".'
		act 'Enter': gt 'rolanapt','hallway' 
	else
		'No one answer. Probably Rolan is outside.'
		act 'Move away from the door':gt 'pavComplexb3', 'etaj_1'
	end
end

if $ARGS[0] = 'hallway':
	CLOSE ALL
	if sound = 0:PLAY 'sound/korr.mp3',30
	$location_type = 'indoors'
	cla
	*clr
	minut += 1
	gs 'stat'
	!gs 'rolanapt' , 'rolanroute'	
	'<center><h4><font color="maroon">Entrance hall</font></h4></center>'
	'<center><img src="images/locations/pavlovsk/resident/apartment/aptrolan/hallway.jpg"></center>'
	if RolanLoc[hour] = 0: 
		'Rolan should be at work'
	!RolanLoc = 0 (Rolan not home school)
	elseif RolanLoc[hour] = 1: 
		'Rolan is a devoted man, he should be in the church'
	!RolanLoc = 1 (Rolan not home church)	
	elseif RolanLoc[hour] = 2: 
		'Rolan is here'
	!RolanLoc = 2 (Rolan in the hallway)	
	elseif RolanLoc[hour] = 3: 
		'You hear snoring...Rolan should be sleeping'
	!RolanLoc = 3 (Rolan in his room)	
	elseif RolanLoc[hour] = 4: 
		'Sniff sniff....somebody is cooking....'
	!RolanLoc = 4 (Rolan in the kitchen)	
	elseif RolanLoc[hour] = 5: 
		'The old tv is on'
	!RolanLoc = 5 (Rolan in the livingroom)	
	elseif RolanLoc[hour] = 6: 
		'There''s a suspicious silence....'
	!RolanLoc = 6 (Rolan in grandma''s room)	
	elseif RolanLoc[hour] = 7: 
		'The water is flowing...you should not disturb Rolan'
	!RolanLoc = 7 (Rolan in the bathroom)	
	elseif RolanLoc[hour] = 8: 
		'The guest room''s door is open, Rolan should be there'
	!RolanLoc = 8:(Rolan in the guestroom)	
	elseif RolanLoc[hour] = 9: 
		'There''s a good weather, probably Rolan is in the park or in the courtyard'
	!RolanLoc = 9 (Rolan not home park)	
    elseif RolanLoc[hour] = 10:
		'There''s a good weather, probably Rolan is in the courtyrard or in the park'
    !RolanLoc = 10 (Rolan not home coutyard)	
    else
		'Where is Rolan?'
	end
	
	if rolan_bazar_day ! daystart: rolan_bazar_count = 5
	if rolan_kuhbaz_day ! daystart: rolan_kuhbaz_count = 5
	if rolan_panty_day ! daystart: rolan_panty_count = 1
	if rolan_steal_day ! daystart: rolan_steal_count = 0

	act 'Room': gt 'rolanapt','room'
	act 'Bathroom': gt 'rolanbath','start'
	act 'Guest room': gt 'rolanapt','guestroom'
	act 'Living Room': gt 'rolanapt','livingroom'
	act 'Kitchen': gt 'rolanapt','kitchen' 
	act 'Grandmaroom': gt 'rolanapt','grandmaroom'
	if rolanworker = 4 and rolanknow = 1:
		RolanLoc[hour] = 6
		gs 'stat'
		'You should check fo Rolan, he seems really upset....'
		act 'Leave Rolan''s apartment': 
			gt 'rolanapt', 'hallway'
		end
	else
		act 'Leave Rolan''s apartment': gt 'pavComplexb3', 'etaj_1'
	end
end

if $ARGS[0] = 'room':
	CLOSE ALL
	cla
	*clr
	minut += 1
	gs 'stat'
	'<center><h4><font color="maroon">Rolan''s bedroom</font></h4></center>'
	'<center><img src="images/locations/pavlovsk/resident/apartment/aptrolan/bedroom.jpg"></center>'
	'This is Rolan''s bedroom, it''s quite the mess. A clock on the wall marks the time, and you see what seems to be an old bedpans under the bed "...is he really so lazy?" you wonder...'
	if RolanLoc[hour] = 3: 'Rolan is here'
	if RolanLoc[hour] ! 3 and rolanpantyQW >= 5: 
		act 'snoop around': gt 'rolanapt','drawer'
	end
	if RolanLoc[hour] = 3 and rolanpantyQW >= 7 and rolanpantyfound = 1 and rolanpantyconfession ! 1 and hour < 23 and $pantyworntype ! 'none': 
		act 'confront rolan': gt 'rolanapt','mypanty'
	end
	act 'Leave the room': gt 'rolanapt','hallway'
end

