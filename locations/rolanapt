if $ARGS[0] = 'rolanroute':
	if RolanLocSet ! DayStart:
		RolanLoc[0] = 3
		RolanLoc[1] = 3
		RolanLoc[2] = 3
		RolanLoc[3] = 3
		RolanLoc[4] = 3
		RolanLoc[5] = 3
		RolanLoc[6] = iif(week ! 7, 0, 3)
		RolanLoc[7] = iif(week ! 7, 0, 3)
		RolanLoc[8] = iif(week ! 7, 0, 3)
		RolanLoc[9] = iif(week ! 7, 0, 1)
		RolanLoc[10] = iif(week ! 7, 0, 1)
		RolanLoc[11] = iif(week ! 7, 0, 1)
		RolanLoc[12] = iif(week ! 7, 0, 1)
		if week ! 6 and week ! 7: RolanLoc[13] = 0
		if month >=3 and month =< 11 and week = 6 or week = 7: RolanLoc[13] = rand(9,10) else RolanLoc[13] = rand(2,8)
		if week ! 6 and week ! 7: RolanLoc[14] = 0
		if month >=3 and month =< 11 and week = 6 or week = 7: RolanLoc[14] = rand(9,10) else RolanLoc[14] = rand(2,8)
		RolanLoc[15] = rand(4,7)
		if week ! 6 and week ! 7: RolanLoc[16] = rand(2,8)
		if month >=3 and month =< 11 and week = 6 or week = 7: RolanLoc[16] = rand(9,10) else RolanLoc[16] = rand(2,8)
		if week ! 6 and week ! 7: RolanLoc[17] = rand(2,8)
		if month >=3 and month =< 11 and week = 6 or week = 7: RolanLoc[17] = rand(9,10) else RolanLoc[17] = rand(2,8)
		if month >=3 and month =< 11: RolanLoc[18] = rand(9,10) else RolanLoc[18] = rand(2,8)
		if week ! 6: RolanLoc[19] = rand(2,8) else RolanLoc[19] = 1
		if week ! 6: RolanLoc[20] = rand(2,8) else RolanLoc[20] = 1
		RolanLoc[21] = rand(4,7)
		RolanLoc[22] = 3
		RolanLoc[23] = 3
		RolanLocSet = DayStart
	end
end

