end

if $ARGS[0] = 'old':
	killobj
	oldobjmenu = 1
	addobj '<font color = green>Character</font>'
	addobj 'Skills'
	addobj 'Statistics'
	addobj 'Pain'
	addobj 'Traits'
	addobj 'Fame'
	if pcs_magik > 0:addobj 'Magic'
	addobj 'Face'
	addobj 'Body'
	addobj 'Clothing'
	addobj 'Panties'
	addobj 'Shoes'
	addobj 'Tattoos'
	addobj 'Piercing'
	addobj 'Phone'
	if bag > 0:addobj 'Purse'
	addobj 'Notebook'
	addobj 'Refresh'
	addobj 'The new menu'
	addobj 'Game Setting'
	addobj 'Cheat menu'
	addobj 'Emergency exit'
	if disable_autosave = 1:
		addobj 'Enable autosave'
	else
		addobj 'Disable autosave'
	end
end

if $ARGS[0] = 'autosave':
	if oldobjmenu = 0:
		if disable_autosave = 1:
			disable_autosave = 0
		else
			disable_autosave = 1
		end
	elseif oldobjmenu = 1:
		if disable_autosave = 1:
			disable_autosave = 0
			delobj 'Enable autosave'
			addobj 'Disable autosave'
		else
			disable_autosave = 1
			delobj 'Disable autosave'
			addobj 'Enable autosave'
		end
	end
	gs'stat'
end
--- obj_din ---------------------------------

