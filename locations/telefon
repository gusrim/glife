			gs 'telefon','telotkaz'
		end
	end

	act 'Invite to the pool hall':
		cla
		'"Let''s play pool."'

		if telotvet > 3:
			'<<$loverdesc[1]>>, "See you at the pool hall in an hour."'

			act 'Go':gt 'svidboy', 'billsvid'
