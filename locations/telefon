				act 'Go':gt 'svidboy', 'parksvid'
			elseif telotvet <= 3:
				gs 'telefon','telotkazboy'
			end
		end
	end

	act 'Invite to a movie':
		cla
		'"Want to go see a movie?"'

		if telotvet > 3:
			'<<$loverdesc[1]>>, "See you at the movie theater in an hour."'

			act 'Go':gt 'svidboy', 'kinosvid'
