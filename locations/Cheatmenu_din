	if pain['head'] < 0:pain['head'] = 0
	if pain['hair'] < 0:pain['hair'] = 0
	if pain['ears'] < 0:pain['ears'] = 0
	if pain['eyebrows'] < 0:pain['eyebrows'] = 0
	if pain['eyes'] < 0:pain['eyes'] = 0
	if pain['cheeks'] < 0:pain['cheeks'] = 0
