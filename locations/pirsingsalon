		*p '<a href="exec:n = <<n>> & gt ''pirsingsalon'', ''shoulder_get''"><img src="images/body/shoulder/tatshldr<<n>>.jpg" height="250" /></a>'
		n += 1
		if n <= 16:jump 'looptatlech'
		*nl

	else
		'<center><img src="images/body/shoulder/tatshldr<<tatlech>>.jpg"></center>'
		'You already have this tattoo here.'
	end

	act 'Return': gt 'pirsingsalon', 'start'
end

if $ARGS[0] = 'shoulder_get':
	*clr
	cla
	'<center><img src="images/body/shoulder/tatshldr<<n>>.jpg"></center>'

	if money < 8000:
		'You cannot afford this tattoo.'
	else
		act 'Get this tattoo':
			cla
			tatlech = n
			money -= 8000
			minut += 60
			gs 'stat'

			act 'Return': gt 'pirsingsalon', 'start'
		end
	end

	act 'Return': gt 'pirsingsalon', 'start'
end

