if $ARGS[0] = 'arm':
	*clr
	cla
	if tatarm = 0:

		n = 1
		:looptatarm
		*p '<a href="exec:n = <<n>> & gt ''pirsingsalon'', ''arm_get''"><img src="images/body/arms/tatarm<<n>>.jpg" height="250" /></a>'
		n += 1
		if n <= 11:jump 'looptatarm'
		*nl

	else
		'<center><img src="images/body/arms/tatarm<<tatarm>>.jpg"></center>'
		'You already have this tattoo here.'
	end

	act 'Return': gt 'pirsingsalon', 'start'
end

if $ARGS[0] = 'arm_get':
	*clr
	cla
	'<center><img src="images/body/arms/tatarm<<n>>.jpg"></center>'

	if money < 8000:
		'You cannot afford this tattoo.'
	else
		act 'Get this tattoo':
			cla
			tatarm = n
			money -= 8000
			minut += 60
			gs 'stat'

			act 'Return': gt 'pirsingsalon', 'start'
		end
	end

	act 'Return': gt 'pirsingsalon', 'start'
end

