! sobsno a subject
$location_type = 'event'
if $args[0] = 'tamara':
	*clr & cla
	'<center><h4><font color="maroon">Tamara Mikhailovna</font></h4></center>'
	'<center><img src="images/qwest/alter/meynolds/tamara.jpg"></center>'
	func('mey_tamara_strings', 'description')

	if $curloc = 'bank': act 'To go home': minut += 30 & gt 'mey_home', 0 
	if $curloc = 'mey_home' and mey_vika['mey_vika_qw'] >= 32 and mey_tamara['qw_end'] = 0:
		if week >= 6 or kanikuli > 0: act 'Ask to go to the city': minut += 30 & gt 'down'
		if GorSlut >= 3 and mey_tamara['reputation_qw'] = 0: act 'To talk about the fall': gt 'mey_tamara_events', 'reputation'
	end
	act 'Depart': gt $curloc, iif($curloc = 'mey_home',mey_loc_id,$metka)
end

! the first meeting
if $args[0] = 'first_meet':
	*clr & cla
