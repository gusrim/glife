end

! the second tea party
if $args[0] = 'next_meet':
	*clr & cla
	minut += 1
	zz_stage = args[1]
	mey_vika['qw_day'] = day
	mey_vika['mey_vika_qw'] = 30
	gs 'stat'
	if zz_stage < 1:
		'<center><img src="images/qwest/alter/meynolds/tamara_tea0.jpg"></center>'
		func('mey_tamara_strings', 'next_tea0')

		act '...': gt 'mey_tamara_events', 'next_meet', 1
	else
		if zz_stage < 8:
			'<center><img src="images/qwest/alter/meynolds/tamara_tea' + zz_stage + '.jpg"></center>'
			func('mey_tamara_strings', 'next_tea' + zz_stage)

			act '...': gt 'mey_tamara_events', 'next_meet', zz_stage + 1
		else
			energy = 20
			water = 20
			manna += 20
			hour = 22
			minut = rand(0,59)
			gs 'stat'
			func('mey_tamara_strings', 'next_tea8')

			killvar 'zz_stage'
			act 'Go': gt 'pavResidential'
			act 'Stay the night': gt 'mey_home'
		end
	end
end

! training, beginning
