		minut += 10
		gs 'stat'
		'<center><img src="images/qwest/alter/meynolds/party9.jpg"></center>'
		func('mey_tamara_strings', 'party14')

		if alko >= 3:
			if mesec = 0 and vagina > 0 and mey_tamara['drink_qw'] = 0:
				act 'Looking for adventure':
					minut += rand(30,40) 
					gs 'stat'
					gt 'mey_tamara_events', 'party4'
				end
			else
				act 'Go out to the car to sleep it off': gt 'mey_tamara_events', 'party3'
			end
		else
			if mey_tamara['builder_qw'] = 0: gt 'mey_tamara_events', 'party5'
			if mey_tamara['bronson_qw'] = 0: gt 'mey_tamara_events', 'party6'
			gt 'mey_tamara_events', 'party2'
		end
	end
	if hour = 23 or hour < 2: act 'To go home': minut += 30 & gt 'mey_home', 0
end

