! falling reputation
if $args[0] = 'reputation':
	cls
	zz_stage = args[1]
	'<center><img src="'+'images/qwest/alter/meynolds/reputation'+zz_stage+iif(zz_stage!3,'.jpg','.gif')+'"></center>'
	func('mey_tamara_strings', 'reputation'+zz_stage)

	if zz_stage < 11:
		act '...': gt 'mey_tamara_events', 'reputation', zz_stage+1
		exit
	else
		killvar 'zz_stage'
		act 'Go':
			mey_tamara['reputation_qw'] = 1
			minut += rand(20,40)
			horny = 0
			spanked = 2
			gt 'mey_home', 3
		end
	end
end
--- mey_tamara_events ---------------------------------

