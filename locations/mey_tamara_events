				'<center><img src="images/qwest/alter/meynolds/learning10.jpg"></center>'
				func('mey_tamara_strings', 'learning15')

				if vnesh < 15: 
					func('mey_tamara_strings', 'learning16')
				end

				if vnesh >= 15 and vnesh < 30: 
					func('mey_tamara_strings', 'learning17')
				end

				if vnesh >= 30 and vnesh < 50: 
					func('mey_tamara_strings', 'learning18')
				end

				if vnesh >= 50: 
					func('mey_tamara_strings', 'learning19')
				end

				act 'Go into the city': gt 'mey_tamara_events', 'learning1'
			end
		end
	end
end

! training trip to the city
if $args[0] = 'learning1':
	*clr & cla
	minut += 30
	gs 'stat'
	func('mey_tamara_strings', 'learning25')

	act '...':
		cla
		func('mey_tamara_strings', 'learning26')

		newdress_count = 0
		newdress_loop = 0
		!by random cloth, modify from :markbuyclomother. original kitty pussy *2 (average clothes), set to 1 formal 1 average

		:markbuyclotamaraaverage
		newdress_id = rand(1, 153)
		
		if average[newdress_id] = 0:
			average[newdress_id] = 1
			averageH[newdress_id] = 100
			averageB[newdress_id] = bedra
			*P '<center><img src="images/clothing/2average/average<<newdress_id>>.jpg"></center>'
		else
			newdress_loop += 1
			if newdress_loop > 1000:
				msg 'ERROR: average cloth loop > 1000'
			else
				jump 'markbuyclotamaraaverage'
			end
		end

		:markbuyclotamaraformal
		newdress_id = rand(1, 14)
		
		if formal[newdress_id] = 0:
			formal[newdress_id] = 1
			formalH[newdress_id] = 35
			formalB[newdress_id] = bedra
			*P '<center><img src="images/clothing/4formal/formal<<newdress_id>>.jpg"></center>'
		else
			newdress_loop += 1
			if newdress_loop > 1000:
				msg 'ERROR: formal cloth loop > 1000'
			else
				jump 'markbuyclotamaraformal'
			end
		end

		
		act '...':
			cla
			func('mey_tamara_strings', 'learning20')

			act 'Beauty salon':
				cla
				func('mey_tamara_strings', 'learning29')

				tan = 10
				resnic = 2
				skin = 4
				if shorthair = 0: curly = 7
				brow = 10
				lobok = 0
				lipbalmstat = 1
				mop = 3
				hour = 19
				minut = 40
				gs 'stat'
				act 'Go further':
					cla
					func('mey_tamara_strings', 'learning21')

					!check if school uniform exist
					newdress_loop = 1
					newdress_id = 0
					:looptamaraschool
					if school[newdress_loop] = 1:
						newdress_id += 1
					else
						newdress_loop += 1
					end
					
					if newdress_id = 0 and newdress_loop <= 14:jump 'looptamaraschool'					
					
					if newdress_id > 0:
						func('mey_tamara_strings', 'learning23')
						
						killvar 'newdress_id'
						killvar 'newdress_count'
						killvar 'newdress_loop'
						
						mey_vika['mey_vika_qw'] = 36
						act 'I have time': gt 'down'
					else
						cla
						minut += 20
						gs 'stat'
						func('mey_tamara_strings', 'learning22')
						
						newdress_loop = 0
						!by random cloth, modify from :markbuyclomother. school uniform in scene
						:markbuyclotamaraschool
						newdress_id = rand(1, 14)
						
						if school[newdress_id] = 0:
							school[newdress_id] = 1
							schoolH[newdress_id] = 130
							schoolB[newdress_id] = bedra
							'<center><img src="images/clothing/6school/school<<newdress_id>>.jpg"></center>'
						else	
							newdress_loop += 1
							if newdress_loop > 1000:
								msg 'ERROR: school cloth loop > 1000'
							else
								jump 'markbuyclotamaraschool'
							end						
						end

						killvar 'newdress_id'
						killvar 'newdress_loop'
						
						act 'To go home': mey_vika['mey_vika_qw'] = 40 & minut += 30 & gt 'mey_home', 0
						act 'To stay in the city':
							cla
							func('mey_tamara_strings', 'learning24')

							act 'I have time': mey_vika['mey_vika_qw'] = 36 & gt 'down'
							act 'To go home': mey_vika['mey_vika_qw'] = 40 & minut += 30 & gt 'mey_home', 0
						end
					end
				end
			end
		end
	end
end

