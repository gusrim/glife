if $ARGS[0] = 'gtipe':
	i=1
	:mark_grup
	if grupTipe=grupTipe[i]:
		grupNPC[i]+=rand(25,30)
	else
		grupNPC[i]-=rand(10,20)
	end
	i+=1
	if i<=25:jump'mark_grup'
end

