		tipgbrand = rand(0, 1)

		if tipgbrand = 0:
			'<<$nameBoyfrend[numnpc]>> suggested going to the disco.'
			dynamic $din_gboy_go_to_disco
			act 'Refuse and go home':otnBoyFrend[numnpc] -= 10 & gt 'pavResidential'
		elseif tipgbrand = 1:
			'<<$nameBoyfrend[numnpc]>> offered to go to his home.'
			dynamic $din_gboy_go_to_boyhome
			act 'Refuse and go home':otnBoyFrend[numnpc] -= 10 & gt 'pavResidential'
		end
	elseif otnBoyFrend[numnpc] >= 80 and gnpcSex[numnpc] > 0:
		gdeystboyrand = rand(0, 1)

		if gdeystboyrand = 0:
			'<<$nameBoyfrend[numnpc]>> offered to go to his home.'
			dynamic $din_gboy_go_to_boyhome
			act 'Refuse and go home':otnBoyFrend[numnpc] -= 10 & gt 'pavResidential'
		elseif gdeystboyrand = 1:
			'<<$nameBoyfrend[numnpc]>> offered to go to the disco.'
			dynamic $din_gboy_go_to_disco
			act 'Refuse and go home':otnBoyFrend[numnpc] -= 10 & gt 'pavResidential'
		end

	end
elseif hour >= 23:
	'<<$nameBoyfrend[numnpc]>> offered to take you home.'

	act 'go home':gt 'pavResidential'
end

--- Gnpc2 ---------------------------------

